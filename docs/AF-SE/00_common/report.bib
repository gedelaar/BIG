  year     = {1991},
  pages    = {235--278},
  file     = {Van Gelder and Topor - 1991 - Safety and translation of relational calculus.pdf:/home/gerard/Zotero/storage/8E2DZ9FI/Van Gelder and Topor - 1991 - Safety and translation of relational calculus.pdf:application/pdf}
